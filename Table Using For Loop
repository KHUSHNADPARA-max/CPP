// Print A Table Using For Loop In C++ Language.

// Here Is CODE :-

#include <iostream>
using namespace std;

int main() {
  
  int n;
  cin >> n;

  int ans;
  cout << endl;
  
  for(int i = 1; i <= 10; i++){
    ans = n * i;
    cout << n << " * " << i << " = " << ans << endl;
  }

  return 0;
}
