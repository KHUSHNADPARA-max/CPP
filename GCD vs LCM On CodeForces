// GCD vs LCM On CodeForces In CPP;

#include <bits/stdc++.h>
using namespace std;

// GCD vs LCM Codeforces
// For a=n-3,b=1,c=1 and d=1, we can see that a+b+c+d=n and gcd(a,b)=lcm(c,d)=1

int main() {
	int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        cout << n - 3 << ' ' << 1 << ' ' << 1 << ' ' << 1 <<endl;
    }
	return 0;
}
