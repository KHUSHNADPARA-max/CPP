#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int x;
        cin>>x;
        // your code goes here
        if(x >= 2000)
            cout << "Yes\n";
        else
            cout << "No\n";
    }
	return 0;
}
